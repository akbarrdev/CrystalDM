{
    "system": {
        "redis": {
            "host": "localhost",
            "port": 6379
        },
        "fastifyLogger": false,
        "debug": "true"
    },
    "server": {
        "tcpPort": 443,
        "udpPort": 19132,
        "host": "0.0.0.0",
        "https": {
            "key": "src/cert/key.pem",
            "cert": "src/cert/cert.pem"
        },
        "information": {
            "name": "GTPS",
            "developer": "Akbarrdev",
            "version": "1.0.0"
        }
    },
    "security": {
        "autoBlacklist": {
            "enabled": true,
            "blacklistPath": "blacklisted.json"
        },
        "whitelist": [
            "127.0.0.1",
            "localhost"
        ],
        "geo": {
            "mode": "whitelist",
            "whitelistCountry": [
                "ID",
                "MY",
                "PH"
            ],
            "blacklistCountry": [
                "NG",
                "VN",
                "IN",
                "RU",
                "CN",
                "BR"
            ]
        },
        "protectedPath": [
            "/wp-login.php",
            "/phpmyadmin",
            "/growtopia"
        ],
        "underAttack": {
            "enabled": "true",
            "discord-webhook-report-channel": "YOUR_WEBHOOK",
            "settings": {
                "maxEventLoopDelay": 1000,
                "maxHeapUsedBytes": 300000000,
                "maxRssBytes": 200000000,
                "maxEventLoopUtilization": 0.9,
                "message": "sek cuy lg overloadd",
                "retryAfter": 50
            }
        },
        "rateLimiter": {
            "limit": 10,
            "per": "1 minute",
            "cache": 10000,
            "headers": {
                "x-protected-by": "Akbarrdev Backend - 2024",
                "x-ratelimit-limit": true,
                "x-ratelimit-remaining": true,
                "x-ratelimit-reset": true,
                "retry-after": true
            }
        },
        "ipReputation": {
            "enabled": true,
            "apikey": "YOUR_API_KEY",
            "threshold": 50
        },
        "adaptiveRateLimit": {
            "enabled": true,
            "defaultLimit": 100,
            "minInterval": 1000,
            "cleanupInterval": 600000
        }
    }
}